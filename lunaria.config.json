{
  "$schema": "./node_modules/@lunariajs/core/config.schema.json",
  "repository": {
    "name": "baranadalc/baranada.com",
    "rootDir": "."
  },
  "files": [
    {
      "location": "**/config.ts",
      "pattern": "@lang/@path",
      "type": "universal"
    },
    {
      "location": "**/*.md",
      "pattern": "@lang/@path",
      "type": "universal"
    }
  ],
  "defaultLocale": {
    "label": "English",
    "lang": "en"
  },
  "locales": [
    {
      "label": "한국어",
      "lang": "ko"
    }
  ],
  "outDir": ".vitepress/dist/_translations",
  "ignoreKeywords": ["lunaria-ignore"]
}